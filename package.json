{
    "name": "ev2017",
    "version": "1.0.0",
    "description": "Boilerplate for jekyll based on h5bp and inuit-css",
    "main": "index.html",
    "dependencies": {
        "cash-dom": "^1.3.5",
        "cookies-js": "^1.2.3"
    },
    "devDependencies": {
        "concurrently": "^3.5.0",
        "inuitcss": "^6.0.0-beta.5",
        "webpack": "^3.5.5"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev":
            "concurrently --kill-others -n \"jkyll,wbpck\" -c \"green.inverse,blue.inverse\" \"bundle exec jekyll serve --incremental\" \"webpack --watch\"; exit 0",
        "build": "webpack --optimize-minimize && bundle exec jekyll build"
    },
    "author": "Lukas Hermann",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/lhermann/ev2017.git"
    }
}
